{
    "name": "MUbedaSJ/decorator",
    "type": "symfony-bundle",
    "description": "DecoratorBundle is a crud decoration library/tools help quicly make beautifull CRUD (using macros and services), this bundle for Symfony 2, 3 and 4",
    "keywords": ["decorator","form","crud","datatable","bs","jquery"],
    "homepage": "https://github.com/MUbedaSJ/",
    "license": "GPL",
    "authors": [
        {
            "name": "Michel UBÉDA SAN JOSÉ",
            "email": "michel.ubeda@gmail.com"
        }
    ],
    "support": {
        "email": "michel.ubeda@gmail.com",
        "issues": "https://github.com/MUbedaSJ/decorator/issues",
        "source": "https://github.com/MUbedaSJ/decorator"
    },
    "require": {
        "php": "^5.5.9|>=7.0.8",
        "symfony/framework-bundle": "~3.4 | ^4.0",
        "symfony/config": "~2.8|~3.0|~4.0",
        "symfony/dependency-injection": "~2.8|~3.0|~4.0",
        "symfony/routing": "~2.8|~3.0|~4.0",
        "symfony/yaml": "^3.3",
        "sensio/framework-extra-bundle": "*"
    },
    "autoload": {
        "psr-4": { "MUbedaSJ\\Bundle\\DecoratorBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "master"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true

}
